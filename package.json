  "dependencies": {
    "@nuxt/kit": "^3.0.0",
    "pdfeasy": "^0.3.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "^3.0.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.27.0",
    "nuxt": "^3.0.0"
