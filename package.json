  },
  "version": "1.0.0",
  "repository": "https://github.com/betterwrite/nuxt-pdfeasy",
  "description": "A PDF document generator to your Nuxt application.",
  "engines": {
    "node": ">=14.16"
  },
  "bugs": {
    "url": "https://github.com/betterwrite/nuxt-pdfeasy/issues"
  },
  "keywords": [
    "pdfkit",
    "pdf",
    "typescript",
    "javascript",
    "printing",
    "nuxt3"
  ],
  "license": "MIT",
